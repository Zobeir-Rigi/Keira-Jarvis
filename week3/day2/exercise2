let age = 66;

if ( age >=18 && age <= 20 ){
    console.log( "You've just entered to adulthood!" )
} if ( age >= 21 && age <= 64 ) {
    console.log( "Welcome to the age of opportunity!" )
} if ( age >= 65 ) {
    console.log( "Enjoy the golden Years!" )
} else if ( age < 18 ){
    console.log("Youth is a treasure")
}